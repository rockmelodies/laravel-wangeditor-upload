{
    "name": "rockymelody/laravel-wangeditor-upload",
    "description": "wangEditor for laravel",
    "keywords": ["laravel", "wangEditor", "web editor", "rockymelody", "wysiwyg"],
    "license": "MIT",
    "authors": [
        {
            "name": "raoyc",
            "email": "raoyc2009@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "autoload": {
        "psr-4": {
            "rockymelody\\WangEditor\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "rockymelody\\WangEditor\\EditorServiceProvider"
            ],
            "dont-discover": [
                "barryvdh/laravel-debugbar"
            ]
        }
    },
    "minimum-stability": "dev"
}
